    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Export (Download from Graph Database)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "export",
                "--dburl",
                "http://localhost/fuseki",
                "--repository",
                "Test",
                "--targetfile",
                "exports/export.ttl",
                "--username",
                "admin",
                "--password",
                "root",
                "--format",
                "text/turtle",
                "--triplestore",
                "fuseki"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import (Upload to Graph Database)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "import",
                "--dburl",
                "http://localhost/fuseki",
                "--repository",
                "Test",
                "--apiType",
                "RDF4J",
                "--context",
                "http://ont.enapso.com/test",
                "--baseiri",
                "http://ont.enapso.com/test#",
                "--sourcefile",
                "exports/export.trig",
                "--username",
                "Test",
                "--password",
                "Test",
                "--format",
                "application/x-trig",
                "--triplestore",
                "fuseki"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Server File (Upload to Graph Database)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "importServerFile",
                "--dburl",
                "http://localhost:7200",
                "--repository",
                "Test",
                "--context",
                "http://ont.enapso.com/test",
                "--baseiri",
                "http://ont.enapso.com/test#",
                "--filename",
                "JSONo.n3",
                "--username",
                "admin",
                "--password",
                "root"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Transform",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "transform",
                "--dburl",
                "http://localhost:7200",
                "--repository",
                "Test",
                "--username",
                "Test",
                "--password",
                "Test",
                "--sourcefile",
                "imports/bindings.json",
                "--targetfile",
                "exports/resultset.json",
                "--format",
                "json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Query (SPARQL File)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "query",
                "--dburl",
                "http://localhost:7200",
                "--repository",
                "Test",
                "--queryfile",
                "queries/all.sparql",
                "--targetfile",
                "results/countries.csv",
                "--prefixfile",
                "queries/prefixes.prf",
                "--format",
                "csv"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update (SPARQL File)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "update",
                "--dburl",
                "http://localhost:7200",
                "--repository",
                "Test",
                "--queryfile",
                "queries/createClass.sparql",
                "--prefixfile",
                "queries/prefixes.prf"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Clear Repository (LocalDB Test)",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "clearRepository",
                "--dburl",
                "http://localhost:7200",
                "--repository",
                "Test",
                "--username",
                "admin",
                "--password",
                "root"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Garbage Collection",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "gc",
                "--dburl",
                "http://localhost:7200",
                "--username",
                "admin",
                "--password",
                "root"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Get Resources",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "getResources",
                "--dburl",
                "http://localhost:7200",
                "--username",
                "Test",
                "--password",
                "Test"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Get Repositories",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "getRepositories",
                "--dburl",
                "http://localhost:7200",
                "--username",
                "admin",
                "--password",
                "root"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create Repository",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "createRepository",
                "-d",
                "http://localhost:7200",
                "-u",
                "admin",
                "-p",
                "root",
                "-r",
                "Test",
                "--repotitle",
                "ENAPSO Automated Test Repo",
                "--isShacl"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Delete Repository",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "deleteRepository",
                "-d",
                "http://localhost:7200",
                "-u",
                "admin",
                "-p",
                "root",
                "-r",
                "Test"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create New User",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "createUser",
                "-d",
                "http://localhost:5820",
                "-u",
                "admin",
                "-p",
                "admin",
                "--newusername",
                "MyTestUser",
                "--newpassword",
                "MyTestUser",
                "--triplestore",
                "stardog",
                "-a",
                "[{\"action\":\"CREATE\",\"resource_type\":\"db\",\"resource\":[\"Test\"]}]"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Updated Newly Created User",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "updateUser",
                "-d",
                "http://localhost:7200",
                "-u",
                "admin",
                "-p",
                "root",
                "--newusername",
                "MyTestUser",
                "-a",
                "ROLE_USER READ_REPO_Test READ_REPO_EnapsoDotNetProDemo"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Delete Updated and Newly Created User",
            "program": "${workspaceFolder}/index.js",
            "args": [
                "deleteUser",
                "-d",
                "http://localhost:7200",
                "-u",
                "admin",
                "-p",
                "root",
                "--newusername",
                "MyTestUser"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Auto Upload Ontology",
            "program": "${workspaceFolder}/index.js",
            "args": ["autoUpload", "--configfile", "configfile/configfile.json"]
        }
    ]
}
