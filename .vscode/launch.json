{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Export (Download from GraphDB)",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"export",
				"--dburl", "http://localhost:7200",
				"--repository", "Test",
				"--context", "http://ont.enapso.com/test",
				"--targetfile", "exports/export.ttl",
				"--username", "Test",
				"--password", "Test",
				"--format", "text/turtle",
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Import (Upload to GraphDB)",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"import",
				"--dburl", "http://localhost:7200",
				"--repository", "Test",
				"--context", "http://ont.enapso.com/test",
				"--baseiri", "http://ont.enapso.com/test#",
				"--sourcefile", "imports/test.owl",
				"--username", "Test",
				"--password", "Test",
				"--format",	"application/rdf+xml",
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Transform",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"transform",
				"--sourcefile",	"imports/bindings.json",
				"--targetfile",	"exports/resultset.json",
				"--format", "json",
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Query (SPARQL File)",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"query",
				"--dburl", "http://localhost:7200",
				"--repository", "Dev",
				// "--context", "http://ont.enapso.com/dev",
				// "--baseiri", "http://ont.enapso.com/dev#",				
				"--queryfile", "queries/countries.sparql",
				"--targetfile",	"results/countries.csv",
				"--prefixfile", "queries/prefixes.prf",
				"--format", "csv",
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Clear Repository (LocalDB Test)",
			"program": "${workspaceFolder}/index.js",
			"args": [
				"clearRepository",
				"--dburl", "http://localhost:7200",
				"--repository", "Test",
				"--username", "Test",
				"--password", "Test"
			]
		}
	]
}